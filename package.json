{
  "name": "gaffer-uploader",
  "description": "GitHub Action to upload test reports to Gaffer",
  "version": "0.4.0",
  "author": "Gaffer <support@gaffer.sh>",
  "private": true,
  "homepage": "https://github.com/gaffer-sh/gaffer-uploader",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaffer-sh/gaffer-uploader.git"
  },
  "bugs": {
    "url": "https://github.com/gaffer-sh/gaffer-uploader/issues"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "bundle": "pnpm run format:write && pnpm run package",
    "ci-test": "jest --runInBand --ci",
    "coverage": "make-coverage-badge --output-path ./badges/coverage.svg",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . -c ./.github/linters/.eslintrc.yml",
    "package": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "package:watch": "pnpm run package -- --watch",
    "test": "jest",
    "all": "pnpm run format:write && pnpm run lint && pnpm run test && pnpm run coverage && pnpm run package",
    "release": "bumpp --tag 'v%s' --commit 'chore: release v%s'"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^5.0.1",
    "@actions/core": "^1.10.1",
    "@actions/http-client": "^2.2.1",
    "@actions/io": "^1.1.3",
    "axios": "^1.6.8",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vercel/ncc": "^0.38.1",
    "bumpp": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-escompat": "^3.11.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-i18n-text": "1.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
